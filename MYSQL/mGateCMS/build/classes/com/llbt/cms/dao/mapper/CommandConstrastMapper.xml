<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="com.llbt.cms.dao.CommandConstrastMapper">
	<!-- 查询结果映射 -->
	<resultMap id="searchResultMap" type="com.llbt.cms.entity.CommandConstrast">
		<result property="id" column="ID" />
		<result property="createBy" column="CREATE_BY" />
		<result property="createDate" column="CREATE_DATE" />
		<result property="updateBy" column="UPDATE_BY" />
		<result property="updateDate" column="UPDATE_DATE" />
		<result property="refSystem" column="REF_SYSTEM" />
		<result property="commandId" column="COMMAND_ID" />
		<result property="commandInfo" column="COMMAND_INFO" />
		<result property="cpsCommandId" column="CPS_COMMAND_ID" />
		<result property="cpsCommandInfo" column="CPS_COMMAND_INFO" />
		<result property="isDelete" column="IS_DELETE" />
	</resultMap>
	<!-- 用于select查询公用抽取的列 -->
	<sql id="columns">
		ID,CREATE_BY,CREATE_DATE,UPDATE_BY,UPDATE_DATE,REF_SYSTEM,COMMAND_ID,COMMAND_INFO,CPS_COMMAND_ID,CPS_COMMAND_INFO,IS_DELETE
	</sql>
	<!-- 新增记录 -->
	<insert id="insert" parameterType="com.llbt.cms.entity.CommandConstrast">
		INSERT INTO
		MMS_COMMAND_CONSTRAST (
		ID ,
		CREATE_BY ,
		CREATE_DATE ,
		UPDATE_BY ,
		UPDATE_DATE ,
		REF_SYSTEM ,
		COMMAND_ID ,
		COMMAND_INFO ,
		CPS_COMMAND_ID ,
		CPS_COMMAND_INFO ,
		IS_DELETE
		) VALUES (
		#{id},
		#{createBy},
		#{createDate},
		#{updateBy},
		#{updateDate},
		#{refSystem},
		#{commandId},
		#{commandInfo},
		#{cpsCommandId},
		#{cpsCommandInfo},
		#{isDelete}
		)
	</insert>
	<!-- 单记录删除 -->
	<delete id="delete" parameterType="java.lang.String">
		update MMS_COMMAND_CONSTRAST
		set
		is_delete=1
		where id=#{id}
	</delete>
	<!-- 批量记录删除 -->
	<delete id="deleteByIds" parameterType="java.util.List">
		update MMS_COMMAND_CONSTRAST set is_delete=1 where id in
		<foreach collection="list" item="ids" open="(" separator=","
			close=")">#{ids}</foreach>
	</delete>
	<!-- 单记录更新 -->
	<update id="update" parameterType="com.llbt.cms.entity.CommandConstrast">
		UPDATE MMS_COMMAND_CONSTRAST
		<trim prefix="set" suffixOverrides=",">
			<if test="updateBy != null">
				UPDATE_BY =#{updateBy},
			</if>
			<if test="updateDate != null">
				UPDATE_DATE =#{updateDate},
			</if>
			<if test="refSystem != null">
				REF_SYSTEM =#{refSystem},
			</if>
			<if test="commandId != null">
				COMMAND_ID =#{commandId},
			</if>
			<if test="commandInfo != null">
				COMMAND_INFO =#{commandInfo},
			</if>
			<if test="cpsCommandId != null">
				CPS_COMMAND_ID =#{cpsCommandId},
			</if>
			<if test="cpsCommandInfo != null">
				CPS_COMMAND_INFO =#{cpsCommandInfo},
			</if>
			<if test="isDelete != null">
				IS_DELETE =#{isDelete},
			</if>
		</trim>
		where id=#{id}
	</update>
	<!-- 查询记录BY ID -->
	<select id="getById" resultMap="searchResultMap" parameterType="java.lang.String">
		select
		<include refid="columns" />
		from
		MMS_COMMAND_CONSTRAST where id=#{id}
	</select>
	<!-- 查询所有记录 -->
	<select id="getAll" resultMap="searchResultMap" parameterType="java.lang.String">
		select
		<include refid="columns" />
		from
		MMS_COMMAND_CONSTRAST where is_delete=0 order by create_date desc
	</select>
	<!-- 查询分页记录 -->
	<select id="queryPage" resultMap="searchResultMap"
		parameterType="com.llbt.cms.vo.Page">
		select 
		<include refid="columns" /> 
		from MMS_COMMAND_CONSTRAST where is_delete=0 
		<if test="entity!=null and entity.commandId!=null and entity.commandId!=''">
			and command_id=#{entity.commandId}
		</if>
		order by create_date desc 
		limit #{startLine},#{pageSize}
	</select>

	<select id="getCommandConstrastCount" resultType="int">
		select count(1)
		from MMS_COMMAND_CONSTRAST where is_delete=0
		<if test="entity!=null and entity.commandId!=null and entity.commandId!=''">
			and command_id=#{entity.commandId}
		</if>
	</select>
	<!-- 动态查询条件 -->
	<sql id="searchConditionSQL">
		<trim prefix="where" prefixOverrides="AND |OR ">
			<if test="id != null">
				and ID = #{id}
			</if>
			<if test="createBy != null">
				and CREATE_BY = #{createBy}
			</if>
			<if test="createDate != null">
				and CREATE_DATE = #{createDate}
			</if>
			<if test="updateBy != null">
				and UPDATE_BY = #{updateBy}
			</if>
			<if test="updateDate != null">
				and UPDATE_DATE = #{updateDate}
			</if>
			<if test="refSystem != null">
				and REF_SYSTEM = #{refSystem}
			</if>
			<if test="commandId != null">
				and COMMAND_ID = #{commandId}
			</if>
			<if test="commandInfo != null">
				and COMMAND_INFO = #{commandInfo}
			</if>
			<if test="cpsCommandId != null">
				and CPS_COMMAND_ID = #{cpsCommandId}
			</if>
			<if test="cpsCommandInfo != null">
				and CPS_COMMAND_INFO = #{cpsCommandInfo}
			</if>
			<if test="isDelete != null">
				and IS_DELETE = #{isDelete}
			</if>
		</trim>
	</sql>



</mapper>