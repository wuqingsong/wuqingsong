<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="com.llbt.cms.dao.AppVersionMapper">
	<!-- 查询结果映射 -->
	<resultMap id="searchResultMap" type="com.llbt.cms.entity.AppVersion">
		<result property="id" column="id" />
		<result property="createBy" column="create_by" />
		<result property="createDate" column="create_date" />
		<result property="updateBy" column="update_by" />
		<result property="updateDate" column="update_date" />
		<result property="appName" column="app_name" />
		<result property="versionNo" column="version_no" />
		<result property="downloadUrl" column="download_url" />
		<result property="appDesc" column="app_desc" />
		<result property="upgradeIdentifier" column="upgrade_identifier" />
		<result property="upgradeType" column="upgrade_type" />
		<result property="upgradeInfo" column="upgrade_info" />
		<result property="publishDate" column="publish_date" />
		<result property="isDelete" column="is_delete" />
	</resultMap>
	<!-- 用于select查询公用抽取的列 -->
	<sql id="columns">
		id,create_by,create_date,update_by,update_date,app_name,version_no,download_url,app_desc,upgrade_identifier,upgrade_type,upgrade_info,publish_date,is_delete
	</sql>
	<!-- 新增记录 -->
	<insert id="insert" parameterType="com.llbt.cms.entity.AppVersion">
		INSERT INTO mms_app_version (
		id ,
		create_by ,
		create_date ,
		update_by ,
		update_date ,
		app_name ,
		version_no ,
		download_url ,
		app_desc ,
		upgrade_identifier ,
		upgrade_type ,
		upgrade_info ,
		publish_date ,
		is_delete
		) VALUES (
		#{id},
		#{createBy},
		#{createDate},
		#{updateBy},
		#{updateDate},
		#{appName},
		#{versionNo},
		#{downloadUrl},
		#{appDesc},
		#{upgradeIdentifier},
		#{upgradeType},
		#{upgradeInfo},
		#{publishDate},
		#{isDelete}
		)
	</insert>
	<!-- 单记录删除 -->
	<delete id="delete" parameterType="java.lang.String">
		update mms_app_version set
		is_delete=1
		where id=#{id}
	</delete>
	<!-- 批量记录删除 -->
	<delete id="deleteByIds" parameterType="java.util.List">
		update mms_app_version set is_delete=1 where id in
		<foreach collection="list" item="ids" open="(" separator=","
			close=")">#{ids}</foreach>
	</delete>
	<!-- 单记录更新 -->
	<update id="update" parameterType="com.llbt.cms.entity.AppVersion">
		UPDATE mms_app_version
		<trim prefix="set" suffixOverrides=",">
			<if test="updateBy != null">
				update_by =#{updateBy},
			</if>
			<if test="updateDate != null">
				update_date =#{updateDate},
			</if>
			<if test="appName != null">
				app_name =#{appName},
			</if>
			<if test="versionNo != null">
				version_no =#{versionNo},
			</if>
			<if test="downloadUrl != null">
				download_url =#{downloadUrl},
			</if>
			<if test="appDesc != null">
				app_desc =#{appDesc},
			</if>
			<if test="upgradeIdentifier != null">
				upgrade_identifier =#{upgradeIdentifier},
			</if>
			<if test="upgradeType != null">
				upgrade_type =#{upgradeType},
			</if>
			<if test="upgradeInfo != null">
				upgrade_info =#{upgradeInfo},
			</if>
			<if test="publishDate != null">
				publish_date =#{publishDate},
			</if>
			<if test="isDelete != null">
				is_delete =#{isDelete},
			</if>
		</trim>
		where id=#{id}
	</update>
	<!-- 查询记录BY ID -->
	<select id="getById" resultMap="searchResultMap" parameterType="java.lang.String">
		select
		<include refid="columns" />
		from
		mms_app_version where id=#{id}
	</select>
	<!-- 查询所有记录 -->
	<select id="getAll" resultMap="searchResultMap" parameterType="java.lang.String">
		select
		<include refid="columns" />
		from
		mms_app_version where is_delete=0 order by create_date desc
	</select>
	<!-- 查询分页记录 -->
	<select id="queryPage" resultMap="searchResultMap"
		parameterType="com.llbt.cms.vo.Page">
		select
		<include refid="columns" />
		from mms_app_version where is_delete=0
		order by create_date desc
		limit #{startLine},#{pageSize}
	</select>

	<select id="getAppVersionCount" resultType="int">
		select count(1) from
		mms_app_version where is_delete=0
	</select>
	<!-- 动态查询条件 -->
	<sql id="searchConditionSQL">
		<trim prefix="where" prefixOverrides="AND |OR ">
			<if test="id != null">
				and id = #{id}
			</if>
			<if test="createBy != null">
				and create_by = #{createBy}
			</if>
			<if test="createDate != null">
				and create_date = #{createDate}
			</if>
			<if test="updateBy != null">
				and update_by = #{updateBy}
			</if>
			<if test="updateDate != null">
				and update_date = #{updateDate}
			</if>
			<if test="appName != null">
				and app_name = #{appName}
			</if>
			<if test="versionNo != null">
				and version_no = #{versionNo}
			</if>
			<if test="downloadUrl != null">
				and download_url = #{downloadUrl}
			</if>
			<if test="appDesc != null">
				and app_desc = #{appDesc}
			</if>
			<if test="upgradeIdentifier != null">
				and upgrade_identifier = #{upgradeIdentifier}
			</if>
			<if test="upgradeType != null">
				and upgrade_type = #{upgradeType}
			</if>
			<if test="upgradeInfo != null">
				and upgrade_info = #{upgradeInfo}
			</if>
			<if test="publishDate != null">
				and publish_date = #{publishDate}
			</if>
			<if test="isDelete != null">
				and is_delete = #{isDelete}
			</if>
		</trim>
	</sql>



</mapper>