<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="com.llbt.cms.dao.SystemDictMapper">
	<!-- 查询结果映射 -->
	<resultMap id="searchResultMap" type="com.llbt.cms.entity.SystemDict">
		<result property="id" column="id" />
		<result property="createBy" column="create_by" />
		<result property="createDate" column="create_date" />
		<result property="updateBy" column="update_by" />
		<result property="updateDate" column="update_date" />
		<result property="dictKey" column="dict_key" />
		<result property="dictValue" column="dict_value" />
		<result property="dictName" column="dict_name" />
		<result property="dictSeq" column="dict_seq" />
		<result property="dictDesc" column="dict_desc" />
		<result property="isDelete" column="is_delete" />
	</resultMap>
	<!-- 用于select查询公用抽取的列 -->
	<sql id="columns">
		id,create_by,create_date,update_by,update_date,dict_key,dict_value,dict_name,dict_seq,dict_desc,is_delete
	</sql>
	<!-- 新增记录 -->
	<insert id="insert" parameterType="com.llbt.cms.entity.SystemDict">
		INSERT INTO mms_system_dict (
		id ,
		create_by ,
		create_date ,
		update_by ,
		update_date ,
		dict_key ,
		dict_value ,
		dict_name ,
		dict_seq ,
		dict_desc ,
		is_delete
		) VALUES (
		#{id},
		#{createBy},
		#{createDate},
		#{updateBy},
		#{updateDate},
		#{dictKey},
		#{dictValue},
		#{dictName},
		#{dictSeq},
		#{dictDesc},
		#{isDelete}
		)
	</insert>
	<!-- 单记录删除 -->
	<delete id="delete" parameterType="java.lang.String">
		update mms_system_dict set
		is_delete=1
		where id=#{id}
	</delete>
	<!-- 批量记录删除 -->
	<delete id="deleteByIds" parameterType="java.util.List">
		update mms_system_dict set is_delete=1 where id in
		<foreach collection="list" item="ids" open="(" separator=","
			close=")">#{ids}</foreach>
	</delete>
	<!-- 单记录更新 -->
	<update id="update" parameterType="com.llbt.cms.entity.SystemDict">
		UPDATE mms_system_dict
		<trim prefix="set" suffixOverrides=",">
			<if test="updateBy != null">
				update_by =#{updateBy},
			</if>
			<if test="updateDate != null">
				update_date =#{updateDate},
			</if>
			<if test="dictKey != null">
				dict_key =#{dictKey},
			</if>
			<if test="dictValue != null">
				dict_value =#{dictValue},
			</if>
			<if test="dictName != null">
				dict_name =#{dictName},
			</if>
			<if test="dictSeq != null">
				dict_seq =#{dictSeq},
			</if>
			<if test="dictDesc != null">
				dict_desc =#{dictDesc},
			</if>
			<if test="isDelete != null">
				is_delete =#{isDelete},
			</if>
		</trim>
		where id=#{id}
	</update>
	<!-- 查询记录BY ID -->
	<select id="getById" resultMap="searchResultMap" parameterType="java.lang.String">
		select
		<include refid="columns" />
		from
		mms_system_dict where id=#{id}
	</select>
	<!-- 查询所有记录 -->
	<select id="getAll" resultMap="searchResultMap" parameterType="java.lang.String">
		select
		<include refid="columns" />
		from
		mms_system_dict where is_delete=0 order by create_date desc
	</select>
	<!-- 查询分页记录 -->
	<select id="queryPage" resultMap="searchResultMap"
		parameterType="com.llbt.cms.vo.Page">
		select *
		from (select rownum rw, t.*
		from (select
		<include refid="columns" />
		from mms_system_dict where is_delete=0
		<if test="entity!=null and entity.dictName!=null and entity.dictName!=''">
			and dict_name=#{entity.dictName}
		</if>
		order by create_date desc) t
		where rownum <![CDATA[<=]]>
		#{endLine})
		where rw <![CDATA[>]]>
		#{startLine}
	</select>

	<select id="getSystemDictCount" resultType="int">
		select count(1) from mms_system_dict where is_delete=0
		<if test="entity!=null and entity.dictName!=null and entity.dictName!=''">
			and dict_name=#{entity.dictName}
		</if>
	</select>
	<!-- 动态查询条件 -->
	<sql id="searchConditionSQL">
		<trim prefix="where" prefixOverrides="AND |OR ">
			<if test="id != null">
				and id = #{id}
			</if>
			<if test="createBy != null">
				and create_by = #{createBy}
			</if>
			<if test="createDate != null">
				and create_date = #{createDate}
			</if>
			<if test="updateBy != null">
				and update_by = #{updateBy}
			</if>
			<if test="updateDate != null">
				and update_date = #{updateDate}
			</if>
			<if test="dictKey != null">
				and dict_key = #{dictKey}
			</if>
			<if test="dictValue != null">
				and dict_value = #{dictValue}
			</if>
			<if test="dictName != null">
				and dict_name = #{dictName}
			</if>
			<if test="dictSeq != null">
				and dict_seq = #{dictSeq}
			</if>
			<if test="dictDesc != null">
				and dict_desc = #{dictDesc}
			</if>
			<if test="isDelete != null">
				and is_delete = #{isDelete}
			</if>
		</trim>
	</sql>
	<select id="getDictName" resultMap="searchResultMap"
		parameterType="java.lang.String">
		select
		<include refid="columns" />
		from
		mms_system_dict where IS_DELETE=0
		<if test="_parameter != null and _parameter != ''">
			and dict_Name=#{dictName}
		</if>
	</select>
	<select id="getKey" resultType="int">
		select count(1)
		from mms_system_dict
		where is_delete = 0
		and dict_Key = #{dictKey}
	</select>


</mapper>