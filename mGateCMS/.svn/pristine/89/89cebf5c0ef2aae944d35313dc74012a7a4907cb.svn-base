<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="com.llbt.cms.dao.CodeConstrastMapper">
	<!-- 查询结果映射 -->
	<resultMap id="searchResultMap" type="com.llbt.cms.entity.CodeConstrast">
		<result property="id" column="ID" />
		<result property="createBy" column="CREATE_BY" />
		<result property="createDate" column="CREATE_DATE" />
		<result property="updateBy" column="UPDATE_BY" />
		<result property="updateDate" column="UPDATE_DATE" />
		<result property="refSystem" column="REF_SYSTEM" />
		<result property="errorCode" column="ERROR_CODE" />
		<result property="errorInfo" column="ERROR_INFO" />
		<result property="transFlag" column="TRANS_FLAG" />
		<result property="transInfo" column="TRANS_INFO" />
		<result property="cpsErrorCode" column="CPS_ERROR_CODE" />
		<result property="cpsErrorInfo" column="CPS_ERROR_INFO" />
		<result property="isDelete" column="IS_DELETE" />
	</resultMap>
	<!-- 用于select查询公用抽取的列 -->
	<sql id="columns">
		ID,CREATE_BY,CREATE_DATE,UPDATE_BY,UPDATE_DATE,REF_SYSTEM,ERROR_CODE,ERROR_INFO,TRANS_FLAG,TRANS_INFO,CPS_ERROR_CODE,CPS_ERROR_INFO,IS_DELETE
	</sql>
	<!-- 新增记录 -->
	<insert id="insert" parameterType="com.llbt.cms.entity.CodeConstrast">
		INSERT INTO MMS_CODE_CONSTRAST (
		ID ,
		CREATE_BY ,
		CREATE_DATE ,
		UPDATE_BY ,
		UPDATE_DATE ,
		REF_SYSTEM ,
		ERROR_CODE ,
		ERROR_INFO ,
		TRANS_FLAG ,
		TRANS_INFO ,
		CPS_ERROR_CODE ,
		CPS_ERROR_INFO ,
		IS_DELETE 
		) VALUES (
#{id},
#{createBy},
#{createDate},
#{updateBy},
#{updateDate},
#{refSystem},
#{errorCode},
#{errorInfo},
#{transFlag},
#{transInfo},
#{cpsErrorCode},
#{cpsErrorInfo},
#{isDelete}
		)
	</insert>
	<!-- 单记录删除 -->
	<delete id="delete" parameterType="java.lang.String">
		update MMS_CODE_CONSTRAST set
		is_delete=1
		where id=#{id}
	</delete>
	<!-- 批量记录删除 -->
	<delete id="deleteByIds" parameterType="java.util.List">
		update MMS_CODE_CONSTRAST set is_delete=1 where id in
		<foreach collection="list" item="ids" open="(" separator=","
			close=")">#{ids}</foreach>
	</delete>
	<!-- 单记录更新 -->
	<update id="update" parameterType="com.llbt.cms.entity.CodeConstrast">
		UPDATE MMS_CODE_CONSTRAST
		<trim prefix="set" suffixOverrides=",">
			<if test="updateBy != null">
				UPDATE_BY =#{updateBy},
			</if>
			<if test="updateDate != null">
				UPDATE_DATE =#{updateDate},
			</if>
			<if test="refSystem != null">
				REF_SYSTEM =#{refSystem},
			</if>
			<if test="errorCode != null">
				ERROR_CODE =#{errorCode},
			</if>
			<if test="errorInfo != null">
				ERROR_INFO =#{errorInfo},
			</if>
			<if test="transFlag != null">
				TRANS_FLAG =#{transFlag},
			</if>
			<if test="transInfo != null">
				TRANS_INFO =#{transInfo},
			</if>
			<if test="cpsErrorCode != null">
				CPS_ERROR_CODE =#{cpsErrorCode},
			</if>
			<if test="cpsErrorInfo != null">
				CPS_ERROR_INFO =#{cpsErrorInfo},
			</if>
			<if test="isDelete != null">
				IS_DELETE =#{isDelete},
			</if>
		</trim>
		where id=#{id}
	</update>
	<!-- 查询记录BY ID -->
	<select id="getById" resultMap="searchResultMap" parameterType="java.lang.String">
		select
		<include refid="columns" />
		from
		MMS_CODE_CONSTRAST where id=#{id}
	</select>
	<!-- 查询所有记录 -->
	<select id="getAll" resultMap="searchResultMap" parameterType="java.lang.String">
		select
		<include refid="columns" />
		from
		MMS_CODE_CONSTRAST where is_delete=0 order by create_date desc
	</select>
	<!-- 查询分页记录 -->
	<select id="queryPage" resultMap="searchResultMap"
		parameterType="com.llbt.cms.vo.Page">
		select *
		from (select rownum rw, t.*
		from (select
		<include refid="columns" />
		from MMS_CODE_CONSTRAST where is_delete=0
		<if test="entity!=null and entity.errorCode!=null and entity.errorCode!=''">
			and error_code=#{entity.errorCode}
		</if>
		 order by create_date desc) t
		where rownum <![CDATA[<=]]>
#{endLine})
		where rw <![CDATA[>]]>
#{startLine}	</select>
	
	<select id="getCodeConstrastCount" resultType="int">
		select count(1) from MMS_CODE_CONSTRAST where  is_delete=0
		<if test="entity!=null and entity.errorCode!=null and entity.errorCode!=''">
			and error_code=#{entity.errorCode}
		</if>
	</select>
	<!-- 动态查询条件 -->
	<sql id="searchConditionSQL">
		<trim prefix="where" prefixOverrides="AND |OR ">
			<if test="id != null">
				and ID = #{id}			</if>
			<if test="createBy != null">
				and CREATE_BY = #{createBy}			</if>
			<if test="createDate != null">
				and CREATE_DATE = #{createDate}			</if>
			<if test="updateBy != null">
				and UPDATE_BY = #{updateBy}			</if>
			<if test="updateDate != null">
				and UPDATE_DATE = #{updateDate}			</if>
			<if test="refSystem != null">
				and REF_SYSTEM = #{refSystem}			</if>
			<if test="errorCode != null">
				and ERROR_CODE = #{errorCode}			</if>
			<if test="errorInfo != null">
				and ERROR_INFO = #{errorInfo}			</if>
			<if test="transFlag != null">
				and TRANS_FLAG = #{transFlag}			</if>
			<if test="transInfo != null">
				and TRANS_INFO = #{transInfo}			</if>
			<if test="cpsErrorCode != null">
				and CPS_ERROR_CODE = #{cpsErrorCode}			</if>
			<if test="cpsErrorInfo != null">
				and CPS_ERROR_INFO = #{cpsErrorInfo}			</if>
			<if test="isDelete != null">
				and IS_DELETE = #{isDelete}			</if>
		</trim>
	</sql>
	<select id="countConstrasteErrorCode" resultType="int">
		select COUNT(1) from
    	mms_code_constrast where is_delete = 0 and ERROR_CODE=#{value}
	</select>
	<select id="countConstrastCpsErrorCode" resultType="int">
		select COUNT(1) from
      mms_code_constrast where is_delete = 0 and CPS_ERROR_CODE=#{value}
	</select>



</mapper>