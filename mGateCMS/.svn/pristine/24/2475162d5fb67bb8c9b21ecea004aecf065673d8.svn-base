package com.llbt.cms.entity;

import com.llbt.cms.common.BaseEntity;

/**
 * QuestionAnswer实体对象
 * 
 * @author 修改成自己的名字
 * @version 1.0
 */
public class QuestionAnswer extends BaseEntity {
	private static final long serialVersionUID = -8148304816610300568L;
	/** 问题标题 */
	private String title;
	/** 问题回答富文本内容 */
	private String answer;
	/** 问答描述 */
	private String questionDesc;
	/** 问题分类:例如付款类,交易类,账户类,有待进一步讨论统一 */
	private String questionType;
	/** 删除状态:0未删除 1已删除 */
	private int isDelete;

	/**
	 * @return the title
	 */
	public String getTitle() {
		return this.title;
	}

	/**
	 * @param title
	 *            the title to set
	 */
	public void setTitle(String title) {
		this.title = title;
	}

	/**
	 * @return the answer
	 */
	public String getAnswer() {
		return this.answer;
	}

	/**
	 * @param answer
	 *            the answer to set
	 */
	public void setAnswer(String answer) {
		this.answer = answer;
	}

	/**
	 * @return the questionDesc
	 */
	public String getQuestionDesc() {
		return this.questionDesc;
	}

	/**
	 * @param questionDesc
	 *            the questionDesc to set
	 */
	public void setQuestionDesc(String questionDesc) {
		this.questionDesc = questionDesc;
	}

	/**
	 * @return the questionType
	 */
	public String getQuestionType() {
		return this.questionType;
	}

	/**
	 * @param questionType
	 *            the questionType to set
	 */
	public void setQuestionType(String questionType) {
		this.questionType = questionType;
	}

	/**
	 * @return the isDelete
	 */
	public int getIsDelete() {
		return this.isDelete;
	}

	/**
	 * @param isDelete
	 *            the isDelete to set
	 */
	public void setIsDelete(int isDelete) {
		this.isDelete = isDelete;
	}
}
